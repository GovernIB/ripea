<?xml version='1.0' encoding='UTF-8'?>
<jboss-deployment-structure xmlns="urn:jboss:deployment-structure:1.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<deployment>
		<dependencies>
			<module name="org.keycloak.keycloak-core" export="true"/>
			<module name="org.apache.cxf.impl"/>
			<module name="org.apache.ws.security"/>
			<module name="org.apache.santuario.xmlsec"/>
			<module name="org.bouncycastle"/>
			<module name="io.undertow.core" export="true"/>
			<module name="io.undertow.servlet" export="true"/>
			<module name="io.undertow.websocket" export="true"/>
			<module name="jdk.management"/>
		</dependencies>
	</deployment>
	<ear-subdeployments-isolated>false</ear-subdeployments-isolated>
	<sub-deployment name="ripea-back.war">
		<exclude-subsystems>
			<subsystem name="jsf"/>			
		</exclude-subsystems>
		<dependencies>
			<module name="jdk.management"/>
		</dependencies>
	</sub-deployment>
	<sub-deployment name="ripea-api-interna.war">
		<exclude-subsystems>
			<subsystem name="jsf"/>
		</exclude-subsystems>
	</sub-deployment>
</jboss-deployment-structure>